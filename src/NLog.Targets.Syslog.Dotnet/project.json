{
  "version": "2.2.0-*",
  "buildOptions": {
    "compile": {
      "include": [ "../NLog.Targets.Syslog/**/*.cs" ]
    },
    "warningsAsErrors": false,
    "outputName": "NLog.Targets.Syslog",
    "xmlDoc": true
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "buildOptions": {
        "define": [ "TRACE" ],
        "optimize": true
      }
    }
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": "dnxcore5.0"

    }
  },
  "dependencies": {
    "NETStandard.Library": "1.6.0",
    "NLog": "4.4.0-betaV15",
    "System.Net.Security": "4.0.0"
  }
}
